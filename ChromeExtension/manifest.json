{
  "name": "Impostor",
  "version": "2.1",
  "description": "Let me impersonate someone else",
  "permissions": ["tabs", "cookies", "downloads"], 
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16" : "images/16.png",
      "32" : "images/32.png",
      "48" : "images/48.png",
      "128": "images/128.png"
    }
  }, 
  "icons": {
    "16" : "images/16.png",
    "32" : "images/32.png",
    "48" : "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 3
}
